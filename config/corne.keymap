            label = "XB";
        };

        sf: js {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp S &kp F &kp SPACE>;
            label = "JS";
        };
    };

    keymap {
        compatible = "zmk,keymap";

        default_layer {
            // -----------------------------------------------------------------------------------------
            // |  TAB |  Q  |  W  |  E  |  R  |  T  |   |  Y  |  U   |  I  |  O  |  P  | BKSP |
            // | CTRL |  A  |  S  |  D  |  F  |  G  |   |  H  |  J   |  K  |  L  |  ;  |  '   |
            // | SHFT |  Z  |  X  |  C  |  V  |  B  |   |  N  |  M   |  ,  |  .  |  /  | ESC  |
            //                    | GUI | LWR | SPC |   | ENT | RSE  | ALT |

            display-name = "Base";
            bindings = <
&kp ESC    &kp Q  &kp W  &kp E      &kp R   &kp T        &kp Y       &kp U        &kp I        &kp O    &kp P     &kp BSPC
&kp N5     &kp A  &kp S  &kp D      &kp F   &kp G        &kp H       &kp J        &kp K        &kp L    &kp SEMI  &kp SQT
&kp LSHFT  &kp Z  &kp X  &kp C      &kp V   &kp B        &kp N       &kp M        &kp COMMA    &kp DOT  &kp FSLH  &kp RIGHT_WIN
                         &kp LCTRL  &kp N4  &lt 1 DEL    &lt 2 CAPS  &lt 3 SPACE  &lt 4 ENTER
            >;
        };

        lower_layer {
            // -----------------------------------------------------------------------------------------
            // |  TAB |  1  |  2  |  3  |  4  |  5  |   |  6  |  7  |  8  |  9  |  0  | BKSP |
            // | BTCLR| BT1 | BT2 | BT3 | BT4 | BT5 |   | LFT | DWN |  UP | RGT |     |      |
            // | SHFT |     |     |     |     |     |   |     |     |     |     |     |      |
            //                    | GUI |     | SPC |   | ENT |     | ALT |

            display-name = "Lower";
            bindings = <
&ahk        &kp LC(F6)   &kp LC(UP)      &kp LC(DOWN)     &kp LC(LA(DELETE))  &kp LG(E)        &none          &kp N1     &kp N2     &kp N3      &kp N4  &kp BSPC
&kp LA(N2)  &kp KP_PLUS  &kp KP_MINUS    &kp KP_MULTIPLY  &kp KP_SLASH        &kp EQUAL        &kp DOLLAR     &kp N5     &kp N6     &kp N7      &kp N8  &kp PERCENT
&kp LC(F8)  &kp LC(N1)   &kp LC(LA(N1))  &kp LC(N2)       &kp TAB             &kp LC(LA(X))    &kp RIGHT_ALT  &kp N9     &kp COMMA  &kp PERIOD  &kp N0  &none
                                         &kp LEFT_SHIFT   &none               &none            &none          &kp RCTRL  &kp ENTER
            >;
        };

        raise_layer {
            // -----------------------------------------------------------------------------------------
            // |  TAB |  !  |  @  |  #  |  $  |  %  |   |  ^  |  &  |  *  |  (  |  )  | BKSP |
            // | CTRL |     |     |     |     |     |   |  -  |  =  |  [  |  ]  |  \  |  `   |
            // | SHFT |     |     |     |     |     |   |  _  |  +  |  {  |  }  | "|" |  ~   |
            //                    | GUI |     | SPC |   | ENT |     | ALT |

            display-name = "Raise";
            bindings = <
&none  &kp F1  &kp F2   &kp F3     &kp F4   &kp PSCRN    &none          &q1    &q2    &q3  &ph    &none
&none  &kp F5  &kp F6   &kp F7     &kp F8   &vj          &yjk           &yx    &ym    &fm  &te    &none
&none  &kp F9  &kp F10  &kp F11    &kp F12  &ma          &kp RIGHT_ALT  &xb    &of    &po  &none  &none
                        &kp LCTRL  &sf      &as          &none          &none  &none
            >;
        };

        layer_3 {
            bindings = <
&kp TILDE  &kp COLON        &kp LEFT_PARENTHESIS  &kp RIGHT_PARENTHESIS  &kp CARET      &kp GRAVE    &none              &kp RC(HOME)  &kp PG_UP         &kp PG_DN          &kp RC(END)      &none
&kp AT     &kp EXCLAMATION  &kp HASH              &kp SLASH              &kp AMPERSAND  &excel       &kp RA(BACKSLASH)  &kp HOME      &kp LEFT_BRACKET  &kp RIGHT_BRACKET  &kp END          &kp PERCENT
&kp UNDER  &kp PIPE         &kp LEFT_BRACE        &kp RIGHT_BRACE        &kp BACKSLASH  &word        &kp RIGHT_ALT      &kp LEFT      &kp UP_ARROW      &kp DOWN_ARROW     &kp RIGHT_ARROW  &none
                                                  &yss                   &qq            &jw          &none              &none         &none
            >;
        };

        layer_4 {
            bindings = <
&none  &dw         &dui        &cuo        &ax         &qs              &bt BT_CLR     &bt BT_SEL 0   &bt BT_SEL 1     &bt BT_SEL 2       &bt BT_SEL 3   &none
&none  &bq         &bqsj       &bqdw       &zw         &yd              &none          &kp RC(K)      &kp RS(ENTER)    &kp RC(ENTER)      &m1            &none
&none  &kp LC(N3)  &kp LC(N4)  &kp LC(N5)  &kp LC(N6)  &kp LC(EQUAL)    &kp RIGHT_ALT  &kp RA(GRAVE)  &kp C_VOLUME_UP  &kp C_VOLUME_DOWN  &kp RG(SPACE)  &kp RG(I)
                               &gs         &jt         &sx              &none          &none          &none
            >;
        };
    };
};
