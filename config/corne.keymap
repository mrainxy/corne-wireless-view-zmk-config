        };

        bq: bq {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp SLASH &kp B &kp Q>;
            label = "BQ";
        };

        bqsj: bqsj {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
