            label = "GS";
        };

        jt: jt {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp SLASH &kp J &kp T>;
            label = "JT";
        };

        sx: sx {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp SLASH &kp S &kp X>;
            label = "SX";
        };

        ahk: ahk {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp A &kp H &kp K &kp SPACE>;
            label = "AHK";
        };

        as: as {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp A &kp S &kp SPACE>;
            label = "AS";
        };

        bd: bd {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp B &kp D &kp SPACE>;
            label = "BD";
        };

        excel: excel {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp E &kp X &kp SPACE>;
            label = "EXCEL";
        };

        ma: ma {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp M &kp A &kp SPACE>;
            label = "MA";
        };

        of: mo {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp O &kp F &kp SPACE>;
            label = "MO";
        };

        po: po {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp P &kp O &kp SPACE>;
            label = "PO";
        };

        qq: qq {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp Q &kp Q &kp SPACE>;
            label = "QQ";
        };

        te: te {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp T &kp E &kp SPACE>;
            label = "TE";
        };

        vj: vj {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp V &kp J &kp SPACE>;
            label = "VJ";
        };

        fm: fm {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp F &kp M &kp SPACE>;
            label = "FM";
        };

        word: word {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp W &kp O &kp SPACE>;
            label = "WORD";
        };

        yjk: yjk {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp Y &kp J &kp K &kp SPACE>;
            label = "YJK";
        };

        ym: ym {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp Y &kp M &kp SPACE>;
            label = "YM";
        };

        jw: jw {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp J &kp W &kp SPACE>;
            label = "JW";
        };

        m1: m1 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp N1 &kp N9 &kp N9 &kp N1>;
            label = "M1";
        };

        q1: q1 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp N9 &kp N7 &kp N9 &kp N1 &kp N9 &kp N0 &kp N5 &kp N1 &kp N0>;
            label = "Q1";
        };

        q2: q2 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp N2 &kp N3 &kp N1 &kp N5 &kp N6 &kp N6 &kp N0 &kp N7 &kp N6 &kp N0>;
            label = "Q2";
        };

        q3: q3 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp N2 &kp N0 &kp N7 &kp N0 &kp N4 &kp N5 &kp N0 &kp N3 &kp N6>;
            label = "Q3";
        };

        yd: yd {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp FSLH &kp FSLH &kp Y &kp D>;
            label = "YD";
        };

        ph: ph {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp N1 &kp N7 &kp N8 &kp N5 &kp N8 &kp N5 &kp N8 &kp N3 &kp N9 &kp N8 &kp N0>;
            label = "PH";
        };

        yx: yx {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp SLASH &kp Y &kp X &kp SPACE>;
            label = "YX";
        };

        ax: ax {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
